<%= form_with model: User.new do |form| %>
    <%= label(:user, :username) %>
    <%= form.text_field :username, placeholder: 'bobson' %>
    <%= label(:user, :email) %>
    <%= form.email_field :email, placeholder: 'bobson@gmail.com' %>
    <%= label(:user, :password) %>
    <%= form.password_field :password, placeholder: '123456' %>
    <%= form.submit %>
<% end %>